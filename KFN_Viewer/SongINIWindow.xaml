<Window x:Name="iniBlocksWindow" x:Class="KFN_Viewer.SongINIWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KFN_Viewer"
        mc:Ignorable="d"
        Title="Song.ini" Height="450" Width="800"
        Style="{StaticResource {x:Type Window}}">
    <Grid>
        <ListView x:Name="iniBlocksView" Style="{StaticResource ListViewWithGridViewStyle}" Height="352" Margin="10,42,466,0" VerticalAlignment="Top" Width="316"/>
        <Label Content="Blocks" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ScrollViewer Height="352" Width="432" Margin="350,42,10,26">
            <TextBox x:Name="blockContent" Height="352" TextWrapping="Wrap" VerticalAlignment="Top" Width="432"/>
        </ScrollViewer>
    </Grid>
</Window>
